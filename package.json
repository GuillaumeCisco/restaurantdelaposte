{
    "name": "restaurantdelaposte",
    "productName": "Restaurant de la Poste",
    "author": "Guillaume Cisco <guillaumecisco@gmail.com>",
    "version": "0.0.1",
    "license": "MIT",
    "description": "Restaurant de la Poste (Chez Robert)",
    "repository": {
        "type": "git",
        "url": "https://github.com/GuillaumeCisco/restaurantdelaposte"
    },
    "browserslist": "electron 1.6",
    "engines": {
        "node": ">=8.4.0",
        "npm": ">=5.3.0"
    },
    "scripts": {
        "start": "npm run clean && cross-env NODE_ENV=development babel-watch webpack/ssr/index.js --inspect",
        "start:prod": "npm run build:main && npm run serve",
        "build:main": "npm run build:client && npm run build:server && npm run build:node",
        "build:client": "rimraf build/ssr/client && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/client.js",
        "build:server": "rimraf build/ssr/server && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/server.js",
        "build:node": "cross-env NODE_ENV=production babel webpack/ssr/index.js -o build/ssr/index.js",
        "serve": "cross-env NODE_ENV=production babel-node build/ssr/index.js",
        "dev": "cross-env IS_ELECTRON=1 START_HOT=1 npm run start-renderer-dev",
        "start-main-dev": "cross-env IS_ELECTRON=1 HOT=1 NODE_ENV=development electron -r babel-register --require babel-polyfill ./src/electron/main",
        "start-renderer-dev": "cross-env IS_ELECTRON=1 NODE_ENV=development babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack/electron/renderer.dev.js",
        "start:electron": "cross-env IS_ELECTRON=1 NODE_ENV=production electron ./build/electron/index.js",
        "build:electron": "concurrently \"npm run build-electron-main\" \"npm run build-electron-renderer\"",
        "build-electron-main": "cross-env IS_ELECTRON=1 NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/electron/main.js --colors",
        "build-electron-renderer": "cross-env IS_ELECTRON=1 NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/electron/renderer.prod.js --colors",
        "package": "npm run build:electron && build --publish never",
        "electron-builder": "build --publish never",
        "deploy": "tools/deploy.js",
        "clean": "rimraf build",
        "test": "jest",
        "coverage": "jest --coverage --no-cache --ci --runInBand",
        "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-register \"./src/app/**/*.spec.js\"",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
        "eslint": "./node_modules/.bin/eslint --fix src webpack"
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/test/shimSetup.js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "collectCoverageFrom": [
            "src/**/*.js"
        ]
    },
    "pre-commit": [
        "precommit-msg",
        "test",
        "eslint"
    ],
    "main": "./build/electron/index.js",
    "build": {
        "appId": "org.guillaumecisco.restaurantdelaposte",
        "productName": "Restaurant de la Poste",
        "directories": {
            "buildResources": "resources",
            "output": "release"
        },
        "files": [
            "build/electron/**/*"
        ],
        "linux": {
            "target": [
                "deb",
                "AppImage"
            ]
        },
        "mac": {
            "target": "dmg"
        },
        "win": {
            "target": "nsis"
        },
        "nsis": {
            "artifactName": "${productName}_${version}_win.${ext}"
        },
        "dmg": {
            "artifactName": "${productName}_${version}_mac.${ext}",
            "contents": [
                {
                    "x": 130,
                    "y": 220
                },
                {
                    "x": 410,
                    "y": 220,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        }
    },
    "dependencies": {
        "babel-runtime": "6.26.0",
        "emotion": "8.0.12",
        "fastclick": "1.0.6",
        "google-map-react": "0.26.0",
        "history": "4.7.2",
        "html-webpack-plugin": "2.30.1",
        "lodash": "4.17.4",
        "react": "16.2.0",
        "react-dom": "16.2.0",
        "react-emotion": "8.0.12",
        "react-helmet": "5.2.0",
        "react-json-tree": "0.11.0",
        "react-redux": "5.0.6",
        "react-tap-event-plugin": "3.0.2",
        "react-universal-component": "2.8.0",
