        "lodash": "4.17.5",
        "prop-types": "15.6.0",
        "react": "16.2.0",
        "react-dom": "16.2.0",
        "react-emotion": "9.0.0",
        "react-helmet": "5.2.0",
        "react-redux": "5.0.6",
        "react-tap-event-plugin": "3.0.2",
        "react-universal-component": "2.8.2",
        "recompose": "0.26.0",
        "redux": "3.7.2",
        "redux-actions": "2.2.1",
        "redux-first-router": "1.9.15",
        "redux-first-router-link": "1.4.2",
        "redux-reducers-injector": "0.1.1",
        "redux-saga": "0.16.0",
        "redux-sagas-injector": "0.2.3",
        "transition-group": "0.0.2",
        "webpack-flush-chunks": "1.2.3"
    },
    "devDependencies": {
        "autodll-webpack-plugin": "0.3.8",
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-minify-webpack-plugin": "0.2.0",
        "babel-jest": "22.1.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-es2015-classes": "6.24.1",
        "babel-polyfill": "6.26.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "babel-register": "6.26.0",
        "babel-watch": "2.0.7",
        "browser-sync": "2.23.5",
        "browser-sync-webpack-plugin": "1.2.0",
        "compression": "1.7.1",
        "config": "1.29.2",
        "cookie-parser": "1.4.3",
        "express": "4.16.2",
        "extract-css-chunks-webpack-plugin": "2.0.18",
        "fetch-everywhere": "1.0.5",
        "happypack": "4.0.1",
        "lodash-webpack-plugin": "0.11.4",
        "stats-webpack-plugin": "0.6.2",
        "sw-precache-webpack-plugin": "0.11.4",
        "webpack": "3.10.0",
        "webpack-bundle-analyzer": "2.9.2",
        "webpack-dev-middleware": "2.0.4",
        "webpack-hot-middleware": "2.21.0",
        "webpack-hot-server-middleware": "0.3.2",
        "write-file-webpack-plugin": "4.2.0"
    },
    "scripts": {
        "start": "npm run clean && cd ../../ && cross-env NODE_ENV=development babel-watch webpack/ssr/index.js",
        "start:prod": "npm run build:main && npm run serve",
        "serve": "cd ../../ && cross-env NODE_ENV=production babel-node build/ssr/index.js",
        "build:main": "npm run build:client && npm run build:server && npm run build:node",
        "build:client": "cd ../../ && rimraf build/client && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/client.js && cd -",
        "build:server": "cd ../../ && rimraf build/server && cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --config webpack/ssr/server.js && cd -",
        "build:node": "cd ../../ && cross-env NODE_ENV=production babel webpack/ssr/index.js -o build/ssr/index.js && cd -",
        "clean": "cd ../../ && rimraf build && cd -"
    }
}
